<section id="video-slider-cartridge"><div class="slick-slider-base container">{% if channel.videos|length %}
	
	
		<h1>Watch Videos</h1>
	

	<div class="row slider">
		
		

		{% for item in channel.videos %}
			
			
				
				{% if item.id %}
					<div id="item-{{rowItem.id}}" class="column video-play">
						


	
	


{% if ( '' != item.asset.thumbnailUrl and '' != tvsite_canonical_url(channel, channel.playlist, item) ) %}
	<a href="tvsite_canonical_url(channel, channel.playlist, item)" class="thumbnail">
		<img src="{{ item.asset.thumbnailUrl }}" alt="{{item.title}}">
	</a>
{% endif %}<p>{{item.title|slice(0, DATA.titleLength is defined ? DATA.titleLength : 40)}}</p>
					</div>
				{% endif %}

			

		{% else %}
			no items to display
		{% endfor %}
	</div>

{% endif %}</div/><div class="bootstrap container">


	
	
		  
	
{% if ( channel and channel.videos ) %}


<script>
  TVSite = window.TVSite || {};
  
  	
      
        TVSite[ 'playList' ] = {{channel.videos|json_encode()}};
      
  	
  
</script>


{% endif %}
</div/></section>